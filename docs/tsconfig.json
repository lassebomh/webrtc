{
  "compilerOptions": {
    "checkJs": true,
    "noEmit": true,
    "target": "ES2024",
    "lib": ["ES2024", "DOM", "ES2017.TypedArrays"],
    "allowJs": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "strict": true,
    "types": [],
    "typeRoots": ["./"],
    "erasableSyntaxOnly": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["./**/*.js", "./game/types.d.ts", "./lib/types.d.ts", "../server.d.ts"]
}
